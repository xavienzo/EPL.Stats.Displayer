PlayerGoals <- read.csv('./Data/PlayerGoals.csv')
PlayerInfo <- read.csv('./Data/PlayerInfo_Scraped.csv')
PlayerInfo$MarketValue <- substring(PlayerInfo$MarketValue, 2)
PlayerInfo$MarketValue <- gsub("m", "0000", PlayerInfo$MarketValue) #original strings w/ m have two digits after dot
PlayerInfo$MarketValue <- gsub("Th.", "000", PlayerInfo$MarketValue)
PlayerInfo$MarketValue <- as.numeric(gsub("\\.", "", PlayerInfo$MarketValue))
PlayerInfo$MarketValue <- PlayerInfo$MarketValue/1000000
PlayerInfo <- PlayerInfo %>% filter(!(Position %in% c("Forward","Midfielder","Sweeper","Defender")))
PlayerTable <- left_join(PlayerInfo,PlayerGoals,by=c("Player","Year")) %>% select(-c(X.x,X.y))
PlayerTable$Goals[is.na(PlayerTable$Goals)] <- as.integer(0)
PlayerTable$GamesPlayed[is.na(PlayerTable$GamesPlayed)] <- as.integer(0)
View(PlayerTable)
write.csv(PlayerInfo, "PlayerInfo_clean.csv")
